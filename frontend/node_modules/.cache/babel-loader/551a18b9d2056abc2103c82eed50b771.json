AAUA,IAAA,aAVA,2BAUA;AAAA;;AACA,UAAA,KAAA,UAAA,CAAA,QAAA,IAAA,EAAA,EAAA;AACA,QAAA,KAAA,CACA,GADA,iEACA,KAAA,UAAA,CAAA,QADA,yBAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,cAAA,YAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,YAAA;;AACA,UAAA,KAAA,CAAA,YAAA,CAAA,YAAA;AACA,SANA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,mCAAA;AACA,SATA;AAUA;AACA,KAvBA;AAwBA,IAAA,YAxBA,wBAwBA,YAxBA,EAwBA;AAAA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,MAAA,EAAA,YAAA,CAAA,EADA;AAEA,QAAA,IAAA,EAAA,YAAA,CAAA,IAFA;AAGA,QAAA,SAAA,EAAA,YAAA,CAAA,SAHA;AAIA,QAAA,QAAA,EAAA,YAAA,CAAA,QAJA;AAKA,QAAA,UAAA,EAAA,YAAA,CAAA,KAAA,CAAA,UALA;AAMA,QAAA,KAAA,EAAA,YAAA,CAAA,KAAA,CAAA,KANA;AAOA,QAAA,gBAAA,EAAA,YAAA,CAAA,gBAPA;AAQA,QAAA,KAAA,EAAA,YAAA,CAAA,KARA;AASA,QAAA,QAAA,EAAA,YAAA,CAAA,QATA;AAUA,QAAA,WAAA,EAAA,YAAA,CAAA;AAVA,OAAA;AAaA,MAAA,qBAAA,CAAA,MAAA,CAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,EAAA,GAAA,QAAA,CAAA,IAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OARA;AASA,KA/CA;AAiDA,IAAA,WAjDA,yBAiDA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,EAAA,GAAA,KAAA;AACA,WAAA,UAAA,GAAA,EAAA;AACA;AArDA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"submit-form mt-3 mx-auto\">\r\n    <p class=\"headline\">Buscar e adicionar repositório</p>\r\n\r\n    <div v-if=\"!submitted\">\r\n      <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n        <v-text-field\r\n          v-model=\"repository.language\"\r\n          :rules=\"rules\"\r\n          label=\"Linguagem ex: java\"\r\n          required\r\n        ></v-text-field>\r\n      </v-form>\r\n\r\n      <v-btn \r\n        color=\"primary\" \r\n        class=\"mt-3\"\r\n        :disabled=\"!valid\"\r\n        @click=\"getRepository\">Enviar</v-btn>\r\n    </div>\r\n\r\n    <div v-else>\r\n      <v-card class=\"mx-auto\">\r\n        <v-card-title> Repositório salvo com sucesso! </v-card-title>\r\n\r\n        <v-card-subtitle>\r\n          Clique no botão Adicionar para buscar e adicionar um novo repositório.\r\n        </v-card-subtitle>\r\n\r\n        <v-card-actions>\r\n          <v-btn \r\n            color=\"success\"\r\n            @click=\"newTutorial\" >Adicionar</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RepositoryDataService from \"../services/RepositoryDataService\"\r\nimport axios from \"axios\"\r\n\r\nexport default {\r\n  name: \"add-tutorial\",\r\n  data() {\r\n    return {\r\n      repository: {\r\n        id: null,\r\n        git_id: 0,\r\n        name: \"\",\r\n        full_name: \"\",\r\n        language: \"\",\r\n        avatar_url: \"\",\r\n        owner: \"\",\r\n        stargazers_count: 0,\r\n        forks: 0,\r\n        html_url: \"\",\r\n        description: \"\",\r\n      },\r\n      ok: false,\r\n      submitted: false,\r\n      rules: [\r\n        v => !!v || \"Campo linguagem obrigatório\",\r\n      ],\r\n      valid: true\r\n    }\r\n  },\r\n  methods: {\r\n    validate () {\r\n      this.$refs.form.validate()\r\n    },\r\n    reset () {\r\n      this.$refs.form.reset()\r\n    },\r\n    resetValidation () {\r\n      this.$refs.form.resetValidation()\r\n    },\r\n    getRepository() {\r\n      if (this.repository.language != \"\") {\r\n        axios\r\n          .get(`https://api.github.com/search/repositories?q=language:${this.repository.language}&sort=stars&page=1`)\r\n          .then((response) => {\r\n            let repositories = response.data.items[0]\r\n            console.log(repositories)\r\n            this.saveTutorial(repositories)\r\n          })\r\n          .catch((error) => {\r\n            console.log(error, \"Ocorreu um erro, tente novamente!\")\r\n          })\r\n      }\r\n    },\r\n    saveTutorial(repositories) {\r\n      var data = {\r\n        git_id: repositories.id,\r\n        name: repositories.name,\r\n        full_name: repositories.full_name,\r\n        language: repositories.language,\r\n        avatar_url: repositories.owner.avatar_url,\r\n        owner: repositories.owner.login,\r\n        stargazers_count: repositories.stargazers_count,\r\n        forks: repositories.forks,\r\n        html_url: repositories.html_url,\r\n        description: repositories.description\r\n      }\r\n\r\n      RepositoryDataService.create(data)\r\n        .then((response) => {\r\n          this.repository.id = response.data.id\r\n          console.log(response.data)\r\n          this.submitted = true\r\n        })\r\n        .catch((e) => {\r\n          console.log(e)\r\n        })\r\n    },\r\n\r\n    newTutorial() {\r\n      this.submitted = false\r\n      this.ok = false\r\n      this.repository = {}\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.submit-form {\r\n  max-width: 400px\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}